{
  "name": "tenants_security",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^5.1.5",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "passport": "^0.4.1",
    "passport-github": "^1.1.0",
    "passport-github2": "^0.1.12"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^27.0.3",
    "sqlite3": "^5.0.2",
    "supertest": "^6.1.3"
  },
  "scripts": {
    "start": "cds run",
    "watch": "cds watch",
    "test": "cross-env NODE_ENV=test npx jest --silent",
    "test:watch": "cross-env NODE_ENV=test npx jest --silent --watchAll"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "es2020": true,
      "node": true,
      "jest": true,
      "mocha": true
    },
    "globals": {
      "SELECT": true,
      "INSERT": true,
      "UPDATE": true,
      "DELETE": true,
      "CREATE": true,
      "DROP": true,
      "CDL": true,
      "CQL": true,
      "CXL": true,
      "cds": true
    },
    "rules": {
      "no-console": "off",
      "require-atomic-updates": "off"
    }
  },
  "cds": {
    "requires": {
      "auth": {
        "impl": "auth/authHandler.js"
      }
    }
  }
}
